<h1>編集ページ</h1>
<%= form_with(model: @user,local: true)do |f| %>
　<%= render 'layouts/flash_messages' %>
  <div class='form'>
    <%= f.label :name,'Name' %>
    <%= f.text_field :name, class:'form-control' %>
  </div>

  <div class='form'>
    <%= f.label :email,'Email' %>
    <%= f.email_field :email, class:'form-control' %>
  </div>

  <%= f.submit '編集を完了',class:'btn btn-primary btn-block margin_top' %>

<% end %>